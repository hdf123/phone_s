/* pages/home/home.wxss */
.headers{
  height:88rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background: rgba(0,153,255,1);
}
.headers>navigator{
  width: 678rpx;
	height: 64rpx;
  border-radius: 8rpx;
  background-color: #ffffff;
  position: relative;
}
.headers>navigator>input{
  width:100%;
  height:100%;
  padding:0 10rpx;
}
.search{
  text-align: center;
}
.headers>navigator .icon-sousuo1{
  font-size:30rpx;
  position: absolute;
  top:18rpx;
  left:28%;
}
.search,.icon-sousuo1{
  color:#999999;
}


/* 
内容 
*/
.contents{
  top:88rpx;
  bottom:0;
}
/* 轮播 */
.swiper1{
  height:325rpx;
}
.swiper1 image,.swiper2 image{
  width:100%;
  height:100%;
}
.swiper2{
  width:690rpx;
  height:325rpx;
}
.swiperContainer{
  width:690rpx;
  position: relative;
  margin:0 auto;
}

.imageCountb {
  width:120rpx;
  height:50rpx;
  background-color: rgba(0, 0, 0, 0.3);
  border-radius:40rpx;
  line-height:50rpx;
  color:#fff;
  text-align:center;
  font-size:26rpx;
  position:absolute;
  right:15rpx;
  bottom:30rpx;
}
/* 菜单 */
.menu {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  padding:35rpx 6rpx 0 6rpx;
  box-shadow: 0rpx 3rpx 14rpx 0rpx rgba(153, 153, 153, 0.3);
}
.menu > navigator {
  width: 25%;
  padding-bottom:35rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.menu > navigator > image {
  width:86rpx;
  height:86rpx;
  border-radius: 50%;
}
.menu >navigator > text {
  margin-top:14rpx;
  color: #666666;
  font-size: 24rpx;
}
/* 推荐 */
.nows {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding:40rpx 30rpx 4rpx;
}
.nows >navigator{
  width: 335rpx;
	height: 136rpx;
	background-color: #f5f5f5;
	border-radius: 4rpx;
  margin-bottom:16rpx;
  padding: 23rpx 28rpx;
  display: flex;
  justify-content: space-between;
}
.nows >navigator > view {
  margin-top:10rpx;
}
.nows >navigator> view >text:nth-child(1) {
  color: #333333;
  font-size: 28rpx;
  display:block;
}
.nows >navigator> view >text:nth-child(2) {
  color: #999999;
  font-size:24rpx;
  margin-top:8rpx;
  display:block;
}
.nows >navigator> image{
  width: 90rpx;
	height: 90rpx;
  border-radius: 50%;
  background: red;
}
/* tab1 */
.swiper-tabc,.swiper-tabd{
  width: 666rpx;
	height: 60rpx;
  margin:71rpx auto 0;
  display: flex;
  justify-content: space-between;
}  
.swiper-tab-list{
  height:100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content:space-between;
} 
.swiper-tab-list text{
  color: #333;
  font-size: 32rpx;
} 
.onc>text{
    color: #0099ff;
}
.onc>view{
  width: 60rpx;
	height: 4rpx;
	background-color: #0099ff;
	border-radius: 2rpx;
}
.swiper3{
  width: 100%; 
  height:890rpx;
  padding:18rpx 30rpx 0;
}
.ak{
  height:218rpx;
  padding:36rpx 0;
  display:flex;
  justify-content: space-between;
  border-bottom:1px solid #e5e5e5;
}
.ak >view > view:nth-child(1) {
  width:454rpx;
  font-size: 28rpx;
  line-height:40rpx;
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:2;
}
.ak >view > view:nth-child(2) {
  color: #999999;
  font-size: 22rpx;
  margin-top:10rpx;
}
.ak > image {
  width: 189rpx;
	height: 144rpx;
  border-radius: 4rpx;
  float:right;
}
.preferential>navigator{
  padding:20rpx 0;
  display:flex;
  justify-content: space-between;
  border-bottom:1px solid #f5f5f5;
}
.preferential>navigator:last-child{
  border:none;
}
.preferential>navigator>view:nth-child(1){
  width:500rpx;
  color: #999999;
  font-size: 28rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.preferential>navigator>view:nth-child(2){
  width:100rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.preferential text{
  color: #999999;
  font-size: 24rpx;
}
.btn{
  width: 690rpx;
	height: 90rpx;
	background-color: #f9f9f9;
	border-radius: 4rpx;
  display: flex;
  margin:20rpx auto 0;
}
.btn>view{
  width:100%;
  height:100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.btn>view>text{
  color: #0099ff;
  font-size: 28rpx;
}
.btn>view:nth-child(1)>text:nth-child(2){
  padding:5rpx 0 0 10rpx;
}
.btn>view:nth-child(2)>text:nth-child(2){
  padding:5rpx 0 0 5rpx;
}
.swiper4{
  width: 100%; 
  height:890rpx;
}





