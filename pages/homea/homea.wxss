/* pages/homea/homea.wxss */
.headers{
  height:88rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background: rgba(0,153,255,1);
}
.headers>navigator{
  width: 678rpx;
	height: 64rpx;
  border-radius: 8rpx;
  background-color: #ffffff;
  position: relative;
}
.headers>navigator>input{
  width:100%;
  height:100%;
  padding:0 10rpx;
}
.search{
  text-align: center;
}
.headers>navigator .icon-sousuo1{
  font-size:30rpx;
  position: absolute;
  top:20rpx;
  left:25%;
}
.search,.icon-sousuo1{
  color:#999999;
}


/* 轮播 */
.swiper1{
  height:325rpx;
}
.swiper1 image,.swiper2 image{
  width:100%;
  height:100%;
}
.swiper2{
  width:690rpx;
  height:325rpx;
}
.swiperContainer{
  width:690rpx;
  position: relative;
  margin:0 auto;
}
.imageCountb {
  width:120rpx;
  height:50rpx;
  background-color: rgba(0, 0, 0, 0.3);
  border-radius:40rpx;
  line-height:50rpx;
  color:#fff;
  text-align:center;
  font-size:26rpx;
  position:absolute;
  right:15rpx;
  bottom:30rpx;
}

/* 菜单 */
.menu {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  padding:17rpx 6rpx 17rpx 6rpx;
  box-shadow: 0rpx 3rpx 14rpx 0rpx rgba(153, 153, 153, 0.3);
}
.menu > navigator {
  width: 25%;
  padding:17rpx 0;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.menu > navigator > image {
  width:86rpx;
  height:86rpx;
  border-radius: 50%;
}
.menu >navigator > text {
  margin-top:14rpx;
  color: #666666;
  font-size: 24rpx;
}

/* 推荐 */
.nows {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding:40rpx 30rpx 4rpx;
}
.nows >navigator{
  width: 335rpx;
	height: 136rpx;
	background-color: #f5f5f5;
	border-radius: 4rpx;
  margin-bottom:16rpx;
  padding: 23rpx 28rpx;
  display: flex;
  justify-content: space-between;
}
.nows >navigator > view {
  margin-top:10rpx;
}
.nows >navigator> view >text:nth-child(1) {
  color: #333333;
  font-size: 28rpx;
  display:block;
}
.nows >navigator> view >text:nth-child(2) {
  color: #999999;
  font-size:24rpx;
  margin-top:8rpx;
  display:block;
}
.nows >navigator> image{
  width: 90rpx;
	height: 90rpx;
  border-radius: 50%;
}

/* tab1 */
.swi_box{
  width:100%;
	height:106rpx;
}
.swiper-tabc,.swiper-tabd{
  width:100%;
  padding:71rpx 42rpx 18rpx 42rpx;
  display: flex;
  justify-content: space-between;
} 
.topk{
  position:fixed;
  top:88rpx;
  left:0;
}
.swiper-tab-list{
  height:48rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content:space-between;
} 
.swiper-tab-list text{
  color: #333;
  font-size: 32rpx;
} 
.onc>text{
    color: #0099ff;
}
.onc>view{
  width: 60rpx;
	height: 4rpx;
	background-color: #0099ff;
	border-radius: 2rpx;
}
.swiper3{
  width: 100%; 
  height:890rpx;
}
.swiper3 swiper-item{
  padding:0 30rpx;
}
.swch{
  height:218rpx;
  padding:36rpx 0;
  display:flex;
  justify-content: space-between;
  border-bottom:1px solid #e5e5e5;
}
.swch >view > view:nth-child(1) {
  width:454rpx;
  font-size: 28rpx;
  line-height:40rpx;
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:2;
}
.swch >view > view:nth-child(2) {
  color: #999999;
  font-size: 22rpx;
  margin-top:10rpx;
}
.swch > image {
  width: 189rpx;
	height: 144rpx;
  border-radius: 4rpx;
  float:right;
}
.preferential>navigator{
  padding:20rpx 0;
  display:flex;
  justify-content: space-between;
  border-bottom:1px solid #f5f5f5;
}
.preferential>navigator:last-child{
  border:none;
}
.preferential>navigator>view:nth-child(1){
  width:500rpx;
  color: #999999;
  font-size: 28rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.preferential>navigator>view:nth-child(2){
  width:100rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.preferential text{
  color: #999999;
  font-size: 24rpx;
}

/* 刷新 */
.btn{
  width: 690rpx;
	height: 90rpx;
	background-color: #f9f9f9;
	border-radius: 4rpx;
  display: flex;
  margin:20rpx auto 0;
}
.btn>view{
  width:100%;
  height:100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.btn>view>text{
  color: #0099ff;
  font-size: 28rpx;
}
.btn>view:nth-child(1)>text:nth-child(2){
  padding:5rpx 0 0 10rpx;
}
.btn>view:nth-child(2)>text:nth-child(2){
  padding:5rpx 0 0 5rpx;
}

/* 轮播 */
.swiper4{
  width: 100%;
  height:2000rpx;
}
.swiper4 swiper-item{
  padding-top:38rpx;
}
.over{
  overflow: auto;
}
.scrollb{
  width:100%;
  white-space: nowrap; 
}
.scroll_childb{
  width:274rpx;
  margin-left:20rpx;
  display: inline-block;
  background:#f9f9f9;
  border-radius:4rpx;
}
.scroll_childb:first-child{
  margin-left:30rpx;
}
.scroll_childb:last-child{
  margin-right:30rpx;
}
.scroll_childb>image{
  width:100%;
  height:186rpx;
}
.scroll_childb>view:nth-child(2){
  color: #333333;
  font-size: 28rpx;
  padding-left:24rpx;
}
.scroll_childb>view:nth-child(3){
  color: #999999;
  font-size: 20rpx;
  padding:12rpx 0 14rpx 24rpx;
}


.lists {
  display: flex;
  padding:36rpx 30rpx 26rpx;
  border-bottom: 1px solid #e5e5e5;
}
.lists > image {
  width: 190rpx;
	height: 144rpx;
  border-radius: 4rpx;
}
.lists > view {
  width:466rpx;
  overflow: hidden;
  margin-left:33rpx;
}
.lists > view > view:nth-child(1) {
  max-width: 100%;
  max-height:60rpx;
  font-size: 28rpx;
}
.lists > view > view:nth-child(4) {
  color: #f9351a;
  font-size: 28rpx;
  margin-top: 10rpx;
}
.lists:last-child {
  border: none;
}

.region {
  height:40rpx;
  display: flex;
  align-items: center;
}
.region > view {
  color: #666666;
  font-size: 22rpx;
}
.region > view:nth-child(2) {
  height:16rpx;
  display: flex;
  align-items: center;
  margin-left:10rpx;
  padding-left:10rpx;
  border-left:2rpx solid #e5e5e5;
}
.features {
  display: flex;
}
.features view {
  color: #999999;
  font-size: 20rpx;
  padding:4rpx 8rpx;
  margin-right:10rpx;
  background: whitesmoke;
  border-radius: 4rpx;
}
.features .statea {
  color: #0099ff;
  background: rgba(178, 224, 255, 0.5);
}
.features .stateb {
  color: #ffb956;
  background: rgba(255, 234, 204, 0.5);
}